{
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/jobs/feeds.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "OPTIONS"]
    },
    {
      "src": "/api/feeds",
      "dest": "src/jobs/feeds.js",
      "methods": ["GET"]
    }
  ],
  "crons": [
    {
      "name": "every minute",
      "path": "/api/feeds",
      "schedule": "0 12 * * *"
    }
  ]
}
