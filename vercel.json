{
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "OPTIONS"]
    }
  ],
  "crons": [
    {
      "name": "every minute",
      "path": "/src/jobs/feeds",
      "schedule": "*/3 * * * *"
    }
  ]
}